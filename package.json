{
  "name": "@opengovsg/checkfirst",
  "version": "1.0.0",
  "description": "Calculators and eligibility checkers for Singapore Government policies",
  "main": "build/server/index.js",
  "homepage": "https://checkfirst.gov.sg",
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint --fix ./src",
    "test": "jest",
    "build": "tsc && webpack --mode production",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "dev-server": "ts-node-dev --respawn --transpile-only --inspect -- src/server/index.ts",
    "dev-client": "webpack serve --mode development --host 0.0.0.0 --devtool inline-source-map --hot",
    "start": "node build/server/index.js"
  },
  "author": "Open Government Products, GovTech Singapore (https://open.gov.sg)",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/opengovsg/checkfirst.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/travis-cli": "^11.0.0",
    "@types/convict": "^5.2.2",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/mathjs": "^6.0.11",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.2",
    "concurrently": "^5.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "commitlint --from origin/develop --to HEAD --verbose"
    }
  },
  "lint-staged": {
    "**/*.(js|jsx|ts|tsx)": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@chakra-ui/react": "^1.1.5",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@types/nodemailer": "^6.4.0",
    "@types/react-datepicker": "^3.1.3",
    "ace-builds": "^1.4.12",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "connect-session-sequelize": "^7.1.0",
    "convict": "^6.0.0",
    "css-loader": "^5.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "framer-motion": "^3.2.1",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.20",
    "mathjs": "^8.1.1",
    "minimatch": "^3.0.4",
    "nodemailer": "^6.4.17",
    "otplib": "^12.0.1",
    "react": "^17.0.1",
    "react-ace": "^9.2.1",
    "react-datepicker": "^3.4.0",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.14.2",
    "react-icons": "^4.1.0",
    "react-query": "^3.5.16",
    "react-router-dom": "^5.2.0",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.1",
    "style-loader": "^2.0.0"
  }
}
